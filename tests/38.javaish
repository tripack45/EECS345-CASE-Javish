var x = 0;
var y = x;
var z = y;
while (1 == 1) {
  y = y - x;
  while (2 == 2) {
    z = z - y;
    while (3 == 3) {
      z = z + 1;
      if (z > 8)
        break;
      else
        continue;
    }
    y = y + 1;
    if (y <= 7)
      continue;
    else
      break;
  }
  x = x + 1;
  if (x > 6)
    break;
  else
    continue;
}
return x * 100 + y * 10 + z;